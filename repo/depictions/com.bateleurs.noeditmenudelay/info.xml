<package>
	<id>com.bateleurs.noeditmenudelay</id>
	<name>NoEditMenuDelay</name>
	<version>1.0-5</version>
	<compatibility>
		<firmware>
			<miniOS>5.0</miniOS>
			<otherVersions>unsupported</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies>mobilesubstrate</dependencies>
	<descriptionlist>
		<description>Removes the annoying half-second delay when opening the edit menu to select/copy/etc.</description>
	</descriptionlist>
	<screenshots></screenshots>
	<changelog>
		<change>iOS 5 and 6 compatible</change>
	</changelog>
	<links></links>
</package>